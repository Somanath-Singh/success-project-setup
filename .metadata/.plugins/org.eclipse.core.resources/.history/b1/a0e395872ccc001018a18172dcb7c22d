	<queryString language="SQL">
		<![CDATA[select  tmf.fin_year,tfe.amount ,tfe.sanction_no ,TO_CHAR(tfe.trans_date, 'DD/MM/YYYY') AS trans_date,tfe.object_type,
	sum(tfe.used_allocation_amount) as used_allocation_amount,sum(tfe.used_budget_amount)as used_budget_amount from e_cashbook.t_fnd_expenditure tfe
	left join e_cashbook.t_bdg_budget tbb on tfe.budget_id = tbb.budget_id
	JOIN public.t_mst_finyear tmf ON tbb.finyear_id = tmf.finyear_id 
	group by tfe.trans_date,tfe.object_type,tfe.used_allocation_amount,tfe.amount ,tfe.sanction_no
	,tfe.used_budget_amount,tmf.fin_year]]>
	</queryString>
