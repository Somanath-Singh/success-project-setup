	<queryString language="SQL">
		<![CDATA[select tmf.fin_year,tbb.trans_date,sum(tbb.amount) as total_amount,tbb.object_type
	,tbb.still_approved_amount ,tbb.remaining_budget_amount,twtsf.display_name from e_cashbook.t_bdg_budget tbb
	join public.t_mst_finyear tmf on tbb.finyear_id = tmf.finyear_id 
	join public.t_wfm_txn_stage_forwarded_rule twtsf on tbb .stage_workflow_id = twtsf.forwarded_id 
	group by tbb.stage_workflow_id,tbb.trans_date,tbb.finyear_id,tbb.object_type
	,tbb.still_approved_amount ,tbb.remaining_budget_amount,tmf.fin_year,twtsf.display_name]]>
	</queryString>
