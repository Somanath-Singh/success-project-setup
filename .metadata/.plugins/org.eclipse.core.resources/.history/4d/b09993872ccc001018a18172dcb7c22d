	<parameter name="monthlyAllowanceId" class="java.lang.Long"/>
	<parameter name="yearId" class="java.lang.String"/>
	<parameter name="monthId" class="java.lang.Long"/>
	<parameter name="employmentTypeId" class="java.lang.Long"/>
	<parameter name="allowanceTypeId" class="java.lang.Long"/>
	<parameter name="allowanceType" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT thsm.staff_name || '(' || thsm.staff_code || ')' AS staff_name_with_code,
	thsam.vehicle_no,
	thsam.amount,
	' ' AS staff_signature
FROM t_hr_staff_allowances_map thsam
	JOIN t_hr_monthly_allowance thma ON
	 thma.monthly_allowances_id = thsam.monthly_allowances_id
	JOIN t_hr_staff_master thsm ON
	 thsam.staff_id = thsm.staff_id
	JOIN t_umt_user tuu ON
	 tuu.email = thsm.email
	JOIN t_umt_role tur ON
	 tur.role_id = tuu.primary_role_id
WHERE
	 thsam.monthly_allowances_id = $P{monthlyAllowanceId}
	 AND thma.financial_year = $P{yearId}
	 AND thma.month_id = $P{monthId}
	 AND thma.employment_type_id = $P{employmentTypeId}
	 AND thma.allowance_type_id = $P{allowanceTypeId}
	 AND thsam.amount >0]]>
	</queryString>
