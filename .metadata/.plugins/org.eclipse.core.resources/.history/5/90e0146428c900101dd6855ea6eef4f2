
package com.aashdit.common.model;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;

import javax.persistence.*;
import java.io.Serializable;
import java.util.Date;
import java.util.List;



@Entity
@Table(name="t_mst_city_master")
@Getter
@Setter
@NoArgsConstructor
@AllArgsConstructor
public class CityMaster implements Serializable {
	private static final long serialVersionUID = 1L;

	@Id
	@GeneratedValue(strategy=GenerationType.IDENTITY)
	@Column(name="city_id")
	private Long cityId;

	@Column(name="city_code")
	private String cityCode;

	@Column(name="city_name")
	private String cityName;

	
	@Column(name="city_type")
	private String cityType;
	
	@Column(name="city_status")
	private String cityStatus;
	
	@Column(name="is_aashdit_branch")
	private Boolean aashditBranch;
	
	@Column(name="created_by")
	private Long user;
	
	@Column(name="created_date")
	private Date date;
	

//	@OneToMany(mappedBy="cityMaster")
//	private List<AccountLevel> accountLevels;
	

	@ManyToOne
	@JoinColumn(name="state_id")
	private State stateMaster;
	
	@ManyToOne
	@JoinColumn(name="district_id")
	private District district;


}
