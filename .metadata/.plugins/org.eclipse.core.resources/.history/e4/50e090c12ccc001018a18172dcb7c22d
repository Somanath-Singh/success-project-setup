    $('body').on('focus',".datepicker>input", function(){
        $(this).datepick({
			 dateFormat: 'dd/mm/yyyy',maxDate: new Date(), showOnFocus: true,
			 onClose: function(selected) {
			 	var birthday = +new Date(selected);
					var age = ~~((Date.now() - birthday) / (31557600000));
					var da = age | 0;
					$('#farmerAge').val(da);
					
			 }
		});
    });
    
